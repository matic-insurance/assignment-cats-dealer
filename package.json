{
  "name": "cats",
  "private": true,
  "dependencies": {
    "@rails/webpacker": "3.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^1.6.6",
    "milligram": "^1.3.0",
    "preact": "^8.3.1",
    "preact-compat-enzyme": "^0.2.5",
    "preact-render-spy": "^1.3.0",
    "preact-router": "^2.6.1",
    "preact-test-utils": "^0.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "webpack-dev-server": "2.11.2"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "start": "./bin/webpack-dev-server",
    "test": "jest",
    "test-start": "jest --watch",
    "lint": "npx eslint app/javascript/*"
  },
  "jest": {
    "collectCoverage": false,
    "testPathIgnorePatterns": [
      "<rootDir>/config/webpack/*"
    ],
    "roots": [
      "<rootDir>/node_modules",
      "<rootDir>/app/javascript"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/app/javascript"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/spec/.jest/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/spec/.jest/setupTests.js"
    ],
    "testURL": "http://example.com",
    "clearMocks": true
  }
}
